    "7bc7d4e5-f9ec-49d7-a556-7fc8430232cc": {
      "type": "rich-text",
      "blocks": {
        "1832d483-9015-4688-9ee0-6939174638c0": {
          "type": "heading",
          "settings": {
            "heading": "<p>Welcome to Celeplate Top up & Save More Member's Club!<\/p>",
            "heading_size": "h2"
          }
        },
        "fa802dd6-e4bb-492b-88d3-8e6a5121e1de": {
          "type": "text",
          "settings": {
            "text": "<p>Welcome to the Celeplate Top-Up & Save More Member's Club, designed for those who love high-quality meat and a premium home-cooking experience. As a member, you can enjoy exclusive benefits and save even more with our prepaid credit deals. Join us today to start enjoying the benefits.<\/p><p><\/p>"
          }
        },
        "553d0edc-57cb-4af5-baef-553212c07340": {
          "type": "heading",
          "settings": {
            "heading": "<p>Save More Member's Club Benefits<\/p>",
            "heading_size": "h2"
          }
        },
        "c62240ff-28cf-4ece-9d86-8edb0ba89ac0": {
          "type": "text",
          "settings": {
            "text": "<ul><li>Get extra credit for free when topping up<\/li><li>Early access to our newest arrivals<\/li><li>Early access to limited edition products<\/li><li>Exclusive discounts and offers via email<\/li><li>Exclusive members-only events and experiences<\/li><li>Regular updates on the latest and greatest products in the industry<\/li><\/ul>"
          }
        }
      },
      "block_order": [
        "1832d483-9015-4688-9ee0-6939174638c0",

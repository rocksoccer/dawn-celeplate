    "8da25f4d-78a0-4033-9b89-a0ef73195030": {
      "type": "image-with-text",
      "blocks": {
        "template--16804654383339__8da25f4d-78a0-4033-9b89-a0ef73195030-16825207796ef9a246-0": {
          "type": "heading",

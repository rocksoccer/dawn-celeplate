    "1658260513b99c1963": {
      "type": "rich-text",
      "blocks": {
        "1658260513ab6dd4c6-1": {
          "type": "text",
          "settings": {
            "text": "<p>Keep refrigerated. Once recieved, please consume within 3 days.<\/p><p>Otherwise, freeze the duck in freezer, and take out the frozen duck 48 hours in advance with its breast side up to defrost. Once it's chilled again, please continue cooking with our cooking instructions.<\/p>"
          }
        }

    "1658260349a43e5800": {
      "type": "rich-text",
      "blocks": {
        "16582603491b061838-2": {
          "type": "button",
          "disabled": true,
          "settings": {
            "button_label": "VIEW COOKING INSTRUCTIONS",
            "button_link": "",
            "button_style_secondary": false,
            "button_label_2": "",
            "button_link_2": "",
            "button_style_secondary_2": false
          }
